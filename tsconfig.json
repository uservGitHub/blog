{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": [
      "dom",
      "es7"
    ],
    "inlineSourceMap": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
