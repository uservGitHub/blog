{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": [
      "es7"
    ],
    "inlineSourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
